<div class="container">
  <table class="table table-striped table-bordered">
    <tr>
      <th>Name</th>
      <th>Created Date</th>
    </tr>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.Name %></td>
        <td><%= account.CreatedDate %></td>
      </tr>
    <% end %>
  </table>

</div>
